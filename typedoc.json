{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "./index.ts",
    "./src/arbitrary.ts",
    "./src/compose.ts",
    "./src/law.ts",
    "./src/laws.ts"
  ],
  "out": "../effect-ts-laws-docs",
  "readme": "./README.md",
  "hideGenerator": true,
  "searchInComments": true,
  "searchInDocuments": true,
  "tsconfig": "./tsconfig.src.json",
  "githubPages": true,
  "cleanOutputDir": false,
  "navigation": {
    "includeCategories": true,
    "includeFolders": true
  },
  "visibilityFilters": {},
  "projectDocuments": [
    "./src/arbitrary/README.md",
    "./src/laws/typeclass/concrete/README.md",
    "./src/laws/typeclass/parameterized/README.md",
    "./tests/laws/typeclass/README.md"
  ]
}
