{
  "name": "effect-ts-laws",
  "version": "1.0.0",
  "description": "Law testing using fast-check.",
  "type": "module",
  "author": "Ran Eilam <ran@haag59.com> (https://middle-ages.github.io/)",
  "homepage": "https://github.com/middle-ages/effect-ts-laws",
  "bugs": "https://github.com/middle-ages/effect-ts-laws/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/middle-ages/effect-ts-laws.git"
  },
  "license": "MIT",
  "keywords": [
    "effect-ts",
    "fast-check",
    "law-testing"
  ],
  "scripts": {
    "clean": "dev/clean",
    "lint": "eslint src index.ts tests",
    "lint-fix": "eslint --fix src index.ts tests",
    "typecheck": "tsc --project tsconfig.build.json --noEmit",
    "test-dependencies": "depcruise src index.ts -c .dependency-cruiser.cjs",
    "graph-dependencies": "dev/graph-dependencies",
    "test": "vitest -c vitest.conf.ts --reporter=verbose",
    "coverage": "vitest -c vitest.conf.ts run --reporter=verbose --coverage",
    "test-run": "vitest -c vitest.conf.ts run",
    "build": "tsc --project tsconfig.build.json",
    "run-tsx": "tsx --conditions test",
    "ls-deps": "pnpm ls --prod",
    "ls-deps-dev": "pnpm ls --dev",
    "build-docs": "typedoc index.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@fast-check/vitest": "^0.1.3",
    "@tsconfig/node-lts": "^20.1.3",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.5.1",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "dependency-cruiser": "^16.4.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fast-check": "^3.22.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.0",
    "typedoc": "^0.26.6",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@effect/typeclass": "^0.26.0",
    "effect": "^3.7.0"
  },
  "peerDependencies": {
    "effect": "workspace:^",
    "vitest": "^2.0.5"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "bracketSpacing": false,
    "bracketSameLine": true,
    "endOfLine": "lf"
  },
  "packageManager": "pnpm@9.2.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "test": "./index.ts",
        "import": "./dist/index.js",
        "default": "./index.js"
      }
    },
    "./law": {
      "import": {
        "types": "./src/law.ts",
        "test": "./src/law.js",
        "import": "./dist/src/law.js",
        "default": "./src/law.js"
      }
    },
    "./arbitrary": {
      "import": {
        "types": "./src/arbitrary.ts",
        "test": "./src/arbitrary.js",
        "import": "./dist/src/arbitrary.js",
        "default": "./src/arbitrary.js"
      }
    },
    "./compose": {
      "import": {
        "types": "./src/compose.ts",
        "test": "./src/compose.js",
        "import": "./dist/src/compose.js",
        "default": "./src/compose.js"
      }
    },
    "./laws": {
      "import": {
        "types": "./src/laws.ts",
        "test": "./src/laws.js",
        "import": "./dist/src/laws.js",
        "default": "./src/laws.js"
      }
    }
  },
  "imports": {
    "#law": "effect-ts-laws/law",
    "#arbitrary": "effect-ts-laws/arbitrary",
    "#compose": "effect-ts-laws/compose",
    "#laws": "effect-ts-laws/laws"
  }
}
